<p id="notice"><%= notice %></p>

<h1>Patients</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpf</th>
      <th>Rg</th>
      <th>Cep</th>
      <th>City</th>
      <th>Street</th>
      <th>District</th>
      <th>State</th>
      <th>Number</th>
      <th>Complement</th>
      <th>Nacionality</th>
      <th>Civil status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patients.each do |patient| %>
      <tr>
        <td><%= patient.name %></td>
        <td><%= patient.cpf %></td>
        <td><%= patient.rg %></td>
        <td><%= patient.cep %></td>
        <td><%= patient.city %></td>
        <td><%= patient.street %></td>
        <td><%= patient.district %></td>
        <td><%= patient.state %></td>
        <td><%= patient.number %></td>
        <td><%= patient.complement %></td>
        <td><%= patient.nacionality %></td>
        <td><%= patient.civil_status %></td>
        <td><%= link_to 'Show', patient %></td>
        <td><%= link_to 'Edit', edit_patient_path(patient) %></td>
        <td><%= link_to 'Destroy', patient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient', new_patient_path %>
