<p id="notice"><%= notice %></p>

<h1>Proceedings</h1>

<table>
  <thead>
    <tr>
      <th>Patient</th>
      <th>Medication</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proceedings.each do |proceeding| %>
      <tr>
        <td><%= proceeding.patient %></td>
        <td><%= proceeding.medication %></td>
        <td><%= link_to 'Ver', proceeding %></td>
        <td><%= link_to 'Editar', edit_proceeding_path(proceeding) %></td>
        <td><%= link_to 'Excluir', proceeding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proceeding', new_proceeding_path %>
